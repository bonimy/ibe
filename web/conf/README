NOTES:

# critical step when starting from clean install
# build apachectl.sh from apachectl.sh.mako
configure.py --load=instances/bacchus_5001.json

# after making a change to a running system
apachectl.sh stop
configure.py --load=instances/bacchus_5001.json
apachectl.sh start










